{
    "name": "api-skeletons/api-tools-oauth2-doctrine-identity",
    "description": "Doctrine Enabled AuthenticatedIdentity",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "oauth2",
        "doctrine",
        "apigility",
        "identity"
    ],
    "authors": [
        {
            "name": "Tom H Anderson",
            "email": "tom.h.anderson@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "api-skeletons/oauth2-doctrine": "^4.0",
        "container-interop/container-interop": "^1.1",
        "gianarb/angry": "^0.1",
        "laminas/laminas-dependency-plugin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "php-coveralls/php-coveralls": "^2.2",
        "laminas/laminas-test": "^3.4",
        "laminas-api-tools/api-tools": "^1.4",
        "laminas-api-tools/api-tools-doctrine": "^2.3",
        "laminas/laminas-i18n": "^2.10.1",
        "laminas/laminas-hydrator": "^3.0.2",
        "api-skeletons/coding-standard": "^1.0",
        "phpro/zf-doctrine-hydration-module": "^2.0 || ^3.0"
    },
    "autoload": {
        "psr-4": {
            "Laminas\\ApiTools\\OAuth2\\Doctrine\\Identity\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZFTest\\OAuth2\\Doctrine\\Identity\\": "test/"
        }
    },
    "extra": {
        "zf": {
            "component": "Laminas\\ApiTools\\OAuth2\\Doctrine\\Identity",
            "config-provider": "Laminas\\ApiTools\\OAuth2\\Doctrine\\ConfigProvider"
        }
    },
    "config": {
        "sort-packages": true
    }
}
